{
  "manifest_version": 2,
  "name": "IONISxBetterVideo",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://courses.ionisx.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://www.youtube.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}